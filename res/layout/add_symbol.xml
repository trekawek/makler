<LinearLayout android:layout_width="fill_parent" android:layout_height="fill_parent"
xmlns:android="http://schemas.android.com/apk/res/android" android:padding="3dip">
<TableLayout android:layout_width="wrap_content" android:layout_height="wrap_content">
<TextView android:text="Wprowadź skrót nazwy papieru lub kilka skrótów oddzielonych spacjami." android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
<EditText android:id="@+id/addQuoteSymbols" android:layout_width="wrap_content" android:layout_height="wrap_content"
android:singleLine="true" android:inputType="text|textCapCharacters"></EditText>
<Button android:id="@+id/addQuoteShow" android:text="Pokaż listę"></Button>
</TableLayout>
</LinearLayout>
